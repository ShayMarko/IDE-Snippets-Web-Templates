	"sm::webTemplate1::Layout.css": {
		"prefix": "sm::webTemplate1::Layout.css",
		"body": [
			"",
			".Layout{",
			"    height: 100%;",
			"    box-sizing: border-box; /*fix css size bugs*/",
			"    display: grid;",
			"    grid-template-rows: 10% 80% 10%;",
			"    grid-template-columns: repeat(6,1fr);/* fr = Fraction*/",
			"}",
			"",
			".Layout > header, .Layout > footer{",
			"    grid-column: span 6;",
			"}",
			"",
			".Layout > header{",
			"    background-color: #053742;",
			"    color:white",
			"}",
			"",
			".Layout > footer{",
			"    background-color: p;",
			"}",
			"",
			".Layout > main, .Layout > aside{",
			"    background-color: #39A2DB;",
			"    color: #F0D9E7;",
			"}",
			".Layout > main {",
			"    grid-column: span 5;",
			"    overflow-y: auto;",
			"    display: flex;",
			"    flex-direction: column;",
			"    flex-wrap: nowrap;",
			"    justify-content: flex-start;",
			"    align-content: stretch;",
			"    align-items:center;",
			"}",
			"",
			".Layout,.Layout > *{",
			"    border: 1px solid black;",
			"}",
			"    "
		],
		"description": ""
	},
